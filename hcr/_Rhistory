print(paste(levels(s$учебный.год)[j], "21", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан" & s$форма=="б" & s$целевые==1], na.rm=T)/3, 2), sep=" - "))
