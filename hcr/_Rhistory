print(paste(levels(s$учебный.год)[j], "12", length(s$фио[s$учебный.год==levels(s$учебный.год)[j] & s$форма=="б" & s$целевые==1]), sep=" - "))
print(paste(levels(s$учебный.год)[j], "13", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j] & s$форма=="б" & s$целевые==1], na.rm=T)/3, 2), sep=" - "))
