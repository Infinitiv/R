print(paste(levels(s$учебный.год)[j], "14", length(s$фио[s$учебный.год==levels(s$учебный.год)[j] & s$форма=="д"]), sep=" - "))
print(paste(levels(s$учебный.год)[j], "15", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j] & s$форма=="д"], na.rm=T)/3, 2), sep=" - "))
print(paste(levels(s$учебный.год)[j], "16", length(s$фио[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан"]), sep=" - "))
print(paste(levels(s$учебный.год)[j], "17", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан"], na.rm=T)/3, 2), sep=" - "))
print(paste(levels(s$учебный.год)[j], "18", length(s$фио[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан" & s$форма=="б"]), sep=" - "))
print(paste(levels(s$учебный.год)[j], "19", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан" & s$форма=="б"], na.rm=T)/3, 2), sep=" - "))
print(paste(levels(s$учебный.год)[j], "20", length(s$фио[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан" & s$форма=="б" & s$целевые==1]), sep=" - "))
print(paste(levels(s$учебный.год)[j], "21", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j] & s$причина=="ан" & s$форма=="б" & s$целевые==1], na.rm=T)/3, 2), sep=" - "))
