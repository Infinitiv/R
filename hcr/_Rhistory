print(paste(levels(s$учебный.год)[j], "8", length(s$фио[s$учебный.год==levels(s$учебный.год)[j]]), sep=" - "))
print(paste(levels(s$учебный.год)[j], "9", round(mean(s$summa[s$учебный.год==levels(s$учебный.год)[j]], na.rm=T), 2), sep=" - "))
}
}
